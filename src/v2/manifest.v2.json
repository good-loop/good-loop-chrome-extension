{
	"manifest_version": 2,
 
	"name": "Tabs for Good by Good-Loop",
	"description": "Raise money for your favourite charity by simply browsing the web!",
	"version": "0.2.1",

	"icons": {
		"64": "img/logo.64.png"
	},

	"incognito":"not_allowed",
 
	"chrome_url_overrides" : {
		"newtab": "newtab.html"
	},

	"optional_permissions": [
	  "storage"
	],

	"options_ui": {
		"page": "options.html",
		"open_in_tab": false
	}	

 }
